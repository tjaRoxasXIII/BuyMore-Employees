<%= erb :nav_bar %>

<% Store.all.each do |store|%>
    <h2><%= store.location%></h2>
    <h3> Address: <%= store.address%></h3>
    <br>
    <h4><u>Store Team Members:</u></h4>
        <% store.employees.sort_by {|name| name.full_name}.each do |employee|%>
        <ul>
            <li><a href="/employees/<%=employee.id%>/profile"><%= employee.full_name%></a></li>
        </ul>
        <%end%>
        <br>
<% end %>